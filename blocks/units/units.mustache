{{#items.isNotEmpty}}
  <div class="units">
    {{#header}}
      <div class="units__header">{{{.}}}</div>
    {{/header}}
    <div class="units-list">
      {{#items}}
        <div class="units-item"{{#strMainId}} id="{{.}}"{{/strMainId}}>
          <div class="units-item__title">{{title}}</div>
          <div class="units-item__label">{{label}}</div>
          <!--noindex-->
          {{#phone}}
            <div class="units-item__phone">
              <a href="{{href}}" class="units-item__phone-value" rel="nofollow">{{text}}</a>
              {{#isWhatsapp}}
