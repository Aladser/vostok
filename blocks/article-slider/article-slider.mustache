{{#items.isNotEmpty}}
<div class="article-slider">
    <div class="article-slider-container">
        <button class="article-slider-arrow article-slider-arrow_prev" type="button">
            <svg width="42" height="42" viewBox="0 0 42 42" fill="none" xmlns="http://www.w3.org/2000/svg">
                <circle cx="21" cy="21" r="21" transform="rotate(-180 21 21)" fill="white" fill-opacity="0.9"/>
                <path d="M24.231 16.0676L19.2387 20.9999L24.231 25.943L22.6941 27.4614L16.154 20.9999L22.6941 14.5383L24.231 16.0676Z" fill="#2C3346" fill-opacity="0.9"/>
            </svg>
        </button>
        <div class="article-slider-block swiper">
            <div class="swiper-wrapper article-slider__wrapper">
                {{#items}}
                    <div class="article-slider-slide swiper-slide" >
                        {{#image.src}}
                        <picture class="article-slider-slide-picture">
                            {{#image.mdSrc}}
                            <source media="(max-width: 767px)" srcset="{{.}}"/>
                            {{/image.mdSrc}}
                            {{#image.lgSrc}}
                            <source media="(max-width: 1279px)" srcset="{{.}}"/>
                            {{/image.lgSrc}}
                            <img src="{{.}}" alt="{{image.alt}}" width="{{image.width}}" height="{{image.height}}" class="article-slider-slide__img" loading="lazy">
                        </picture>
                        {{#image.realSrc}}
                        <a href="{{.}}" class="link-as-card" data-fancybox="article-slider-gallery-{{index}}"></a>
                        {{/image.realSrc}}
                        {{/image.src}}
                    </div>
                {{/items}}
            </div>
        </div>
        <button class="article-slider-arrow article-slider-arrow_next" type="button">
            <svg width="42" height="42" viewBox="0 0 42 42" fill="none" xmlns="http://www.w3.org/2000/svg">
                <circle cx="21" cy="21" r="21" fill="white" fill-opacity="0.9"/>
                <path d="M17.769 25.9324L22.7613 21.0001L17.769 16.057L19.3059 14.5386L25.846 21.0001L19.3059 27.4617L17.769 25.9324Z" fill="#2C3346" fill-opacity="0.9"/>
            </svg>
        </button>
    </div>
    {{#thumbs.isNotEmpty}}
        <div class="article-slider-thumbs">
            <button class="article-slider-thumbs-arrow article-slider-thumbs-arrow_prev" type="button">
                <svg width="8" height="13" viewBox="0 0 9 15" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="M1.293 2.707a1 1 0 1 1 1.414-1.414l6 6a1 1 0 0 1 0 1.414l-6 6a1 1 0 0 1-1.414-1.414L6.586 8 1.293 2.707z" fill="#282D3C" /></svg>
            </button>
            <div class="article-slider-thumbs-slider swiper">
                <div class="swiper-wrapper article-slider-thumbs-wrap">
                    {{#thumbs}}
                    <div class="article-slider-thumb swiper-slide">
                        <div class="article-slider-thumb__img-wrap">
                            {{#image.src}}
                            <picture class="article-slider-slide-thumb-picture">
                                {{#image.mdSrc}}
                                <source media="(max-width: 767px)" srcset="{{.}}"/>
                                {{/image.mdSrc}}
                                {{#image.lgSrc}}
                                <source media="(max-width: 1279px)" srcset="{{.}}"/>
                                {{/image.lgSrc}}
                                <img src="{{.}}" alt="{{image.alt}}" width="{{image.width}}" height="{{image.height}}" class="article-slider-thumb__img" loading="lazy">
                            </picture>
                            {{/image.src}}
                            <div class="article-slider-thumb__plague"></div>
                        </div>
                    </div>
                    {{/thumbs}}
                </div>
            </div>
            <button class="article-slider-thumbs-arrow article-slider-thumbs-arrow_next" type="button">
                <svg width="8" height="13" viewBox="0 0 9 15" fill="none" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" clip-rule="evenodd" d="M1.293 2.707a1 1 0 1 1 1.414-1.414l6 6a1 1 0 0 1 0 1.414l-6 6a1 1 0 0 1-1.414-1.414L6.586 8 1.293 2.707z" fill="#282D3C" /></svg>
            </button>
        </div>
    {{/thumbs.isNotEmpty}}
</div>
{{/items.isNotEmpty}}